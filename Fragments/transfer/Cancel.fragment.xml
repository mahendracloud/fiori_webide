<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:fb="sap.ui.comp.filterbar" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form"
	xmlns="sap.m" xmlns:u="sap.ui.unified" xmlns:table="sap.ui.table" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc">
	<Dialog title="Cancel Measurement Reading Transfer" stretch="true">
		<beginButton>
			<Button text="OK" press="onCancelOKPress"></Button>
		</beginButton>
		<endButton>
			<Button text="Cancel" press="onCancelCancelPress"></Button>
		</endButton>
		<content>
			<f:SimpleForm id="cancelForm" minWidth="1024" maxContainerCols="1" editable="true" layout="ResponsiveGridLayout" labelSpanL="3"
				labelSpanM="3" emptySpanL="4" emptySpanM="4" columnsL="1" columnsM="1" class="SFChildPaddingFix">
				<f:content>
					<Label text="Reading Transfer from"></Label>
					<Text id="readTransFr"></Text>
					<Label text="Valid From"></Label>
					<Text id="validFromCancel"></Text>
					<Text id="validTimeCancel"></Text>
					<Label text="Valid To"></Label>
					<Text id="validToCancel"></Text>
					<Text id="validToTimeCancel"></Text>
					<Label text="Valid To New"></Label>
					<DatePicker id="validToNew" displayFormat="dd-MM-yyyy"></DatePicker>
					<Label text="Measurement Position"></Label>
					<Text id="measPosCancel"></Text>
					<Label text="Description"></Label>
					<Text id="descCancel"></Text>
					<Label text="Functional Location"></Label>
					<Text id="flocCancel"></Text>
					<Label text="Description"></Label>
					<Text id="flocDescCancel"></Text>
				</f:content>
			</f:SimpleForm>
		</content>
	</Dialog>
</core:FragmentDefinition>