<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:u="sap.ui.unified" xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<Dialog title="{i18n>filterObjects}" afterClose="onFilterDialogClose">
		<beginButton>
			<Button text="{i18n>ok}" press="onFilterOkPress"></Button>
		</beginButton>
		<endButton>
			<Button text="{i18n>cancel}" press="onFilterCancelPress"></Button>
		</endButton>
		<content>
			<Table>
				<columns>
					<Column>
						<header>
							<Text text="{i18n>type}"></Text>
						</header>
					</Column>
					<Column>
						<header>
							<Text text="{i18n>object}"></Text>
						</header>
					</Column>
					<Column>
						<header>
							<Text text="{i18n>includedInHier}"></Text>
						</header>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<cells>
							<Text></Text>
							<Text></Text>
							<CheckBox></CheckBox>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Dialog>
</core:FragmentDefinition>