			<Title textAlign="Left" level="H2" wrapping="false" text="International Address" class="sapUiSmallMarginBegin"
				visible="{ApproveModel>/approveVisible}">
				<layoutData>
					<l:GridData span="L12 M12 S12"/>
				</layoutData>
			</Title>
			<Table items="{AIWAPPROVE>/intlAddr}" visible="{ApproveModel>/approveVisible}">
				<!--<headerToolbar>-->
				<!--	<Toolbar>-->
				<!--		<Title id="idTTLIntlVersion" text="International Version"/>-->
				<!--		<ToolbarSpacer/>-->
				<!--		<Button id="idBTNnewIntlAddr" icon="sap-icon://add" tooltip="New" press="handleIntlAddrAdd" enabled="{AIWAPPROVE>/addIntlBtn}"/>-->
				<!--	</Toolbar>-->
				<!--</headerToolbar>-->
				<columns>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="Address Version"></Text>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="Name"></Text>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="Name 2"></Text>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="Street"></Text>
					</Column>
					<Column demandPopin="true" minScreenWidth="Tablet">
						<Text text="City"></Text>
					</Column>
				</columns>
				<items>
					<ColumnListItem press="handleIntlAddrPress" type="Navigation">
						<cells>
							<Select forceSelection="false" items="{path: 'AddressVersionModel>/'}" selectedKey="{AIWAPPROVE>AdNation}" width="100%" enabled="false">
								<core:Item key="{AddressVersionModel>Nation}" text="{AddressVersionModel>NationTex}"/>
							</Select>
							<Input maxLength="40" value="{AIWAPPROVE>Name1i}" enabled="false"></Input>
							<Input maxLength="40" value="{AIWAPPROVE>Name2i}" enabled="false"></Input>
							<Input showValueHelp="true" value="{AIWAPPROVE>Streeti}" valueHelpRequest="onStreetVH" change="onStreetIChange" liveChange="onLiveChange"
								enabled="false"></Input>
							<Input showValueHelp="true" value="{AIWAPPROVE>City1i}" valueHelpRequest="onCityVH" change="onCityIChange" liveChange="onLiveChange"
								enabled="false"></Input>
						</cells>
					</ColumnListItem>
				</items>
