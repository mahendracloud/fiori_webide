					<l:GridData span="L2 M2 S4"/>
				</layoutData>
			</Input>
			<Input id="workUnitDesc" enabled="false" visible="{aVisModel>/visible}" value="{wcDetailModel>/Vgarbtxt}"></Input>
			<Label text="{i18n>SCH_TAB_DEL}" visible="{aVisModel>/visible}"></Label>
			<CheckBox id="schTabDel" enabled="false" visible="{aVisModel>/visible}" selected="{wcDetailModel>/Schdelflg}"></CheckBox>
		</f:content>
	</f:SimpleForm>
</core:FragmentDefinition>